{
    "id": 1,
    "optimizer": {
        "lr": 1e-4
    }, 
    "data": {
        "training_data_path": "./data/Space_Time_W_Training.npy",
        "test_data_path": "./data/Space_Time_W_Test.npy"
    },
    "encoder": {
        "activation": "relu",
        "latent_dim": 2,
        "conv_1": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 1
        },
        "max_pool_1": {
            "pool_size": 2,
            "pool_stride": 2
        },
        "conv_2": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 1
        },
        "max_pool_2": {
            "pool_size": 2,
            "pool_stride": 2
        },
        "conv_3": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 1
        },
        "max_pool_3": {
            "pool_size": 2,
            "pool_stride": 2
        },
        "dense_1": {
            "dim": 100
        }
    },
    "decoder": {
        "latent_dim": 2,
        "activation": "relu",
        "conv_t_1": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 2
        },
        "conv_t_2": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 2
        },
        "conv_t_3": {
            "filter_num": 32,
            "kernel_size": 3,
            "stride": 2
        },
        "conv_t_4": {
            "filter_num": 1,
            "kernel_size": 3,
            "stride": 1
        },
        "dense_mu_var": {
            "activation": "linear"
        }
    },
    "batch_size": 128,
    "train_epochs": 60
}